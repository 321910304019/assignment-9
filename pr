return
 li_copy List1=[]
 n=int(input("Enter the size of the list:"))
  for i in range(0, n):
 print("Enter number at location", i, ":")
   item = int(input())
 List1.append(item) 
   print("The list is=",List1)
 List2=Copy(List1) 
print("Original List:",List1)
 print("After Cloning:",List2) 

